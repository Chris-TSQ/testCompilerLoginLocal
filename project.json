{
  "name": "compiler-testgen",
  "displayName": "Compiler Test Generator",
  "description": "Generate Cypress and PyTest tests by selecting a source file",
  "version": "1.0.0",
  "publisher": "tinkersmithfirewaking",    // lowercase, matching publisher account
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": ["Testing", "Other"],
  "activationEvents": ["onCommand:testgen.generate"],
  "main": "extension.js",
  "contributes": {
    "commands": [
      {
        "command": "testgen.generate",
        "title": "Generate Tests for This File"
      }
    ]
  }
}
